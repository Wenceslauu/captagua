generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model umidade_solo {
  id           Int   @id @default(autoincrement())
  umidade_solo Float @db.Float
  fluxo_agua   Float @db.Float
}

model tbl_dates {
  id                Int      @id @default(autoincrement()) @db.SmallInt
  data_hora         DateTime @db.Timestamp(0)
  umidade_solo      Float    @db.Float
  fluxo_agua        Float    @db.Float
  corrente_bateria  Float    @db.Float
  potencia_circuito Float    @db.Float
  valor_economizado Float    @db.Float
}
